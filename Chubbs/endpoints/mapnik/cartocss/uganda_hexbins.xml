<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE Map>

<Map minimum-version="2.0.0">
  <Style name="uganda_hexbins-outline" filter-mode="first" >
    <Rule>
      <LineSymbolizer stroke="#ffffff" stroke-width="1" stroke-linejoin="round" />
    </Rule>
  </Style>
  <Style name="uganda_hexbins" filter-mode="first" >
    <Rule>
      <Filter>([cicoscapit] &gt;= 8.14)</Filter>
      <PolygonSymbolizer fill="#008000" fill-opacity="1" />
      <LineSymbolizer stroke="#559944" stroke-width="0.5" />
    </Rule>
    <Rule>
      <Filter>([cicoscapit] &gt;= 2.385)</Filter>
      <PolygonSymbolizer fill="#ffff00" fill-opacity="1" />
      <LineSymbolizer stroke="#559944" stroke-width="0.5" />
    </Rule>
    <Rule>
      <Filter>([cicoscapit] &gt;= 0.01)</Filter>
      <PolygonSymbolizer fill="#ff0000" fill-opacity="1" />
      <LineSymbolizer stroke="#559944" stroke-width="0.5" />
    </Rule>
    <Rule>
      <LineSymbolizer stroke="#559944" stroke-width="0.5" />
      <PolygonSymbolizer fill-opacity="1" fill="#aaee88" />
    </Rule>
  </Style>

</Map>

